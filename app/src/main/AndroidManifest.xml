<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TweenAnimationActivity"/>
        <activity android:name=".FrameAnimationActivity"/>
        <activity android:name=".PropertyAnimationActivity"/>
        <activity android:name=".PropertyDetialAnimationActivity"/>
        <activity android:name=".ObjectAnimActivity"/>
        <activity android:name=".AnimatorSetActivity"/>
        <activity android:name=".ViewAnimateActivity"/>
        <activity android:name=".ValueAnimatorActivity"/>
        <activity android:name=".LayoutTransitionActivity"/>
    </application>

</manifest>